<title> <%= title %> </title>

<!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"
      integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

<!-- Custom Stylesheets -->
<link rel="stylesheet" href="../assets/css/styles.css">

<% include ./partials/header.ejs%>

<div class="page-header text-center"><img src="../assets/images/mmlogoblack.png" alt=""></div>

<div class="container">

    <form action="/registration" method="POST" id="signup" class="square">

        </br>

        <div class="form-group">
            <label for=""> First Name</label>
            <input type="text" placeholder="Bob" class="form-control" name="firstName" autofocus required>
        </div>

        <div class="form-group">
            <label for="">Last Name</label>
            <input type="text" placeholder="Marley" class="form-control" name="lastName" required>
        </div>

        <div class="form-group">
            <label for="">Phone</label>
            <input type="text" placeholder="555-555-5555" class="form-control" name="phone" required>
        </div>

        <div class="form-group">
            <label for="">E-mail</label>
            <input type="text" placeholder="bob.marley@gmail.com" class="form-control" name="email">
        </div>

        </br>

        <div class="form-group fieldset">
            <button class="btn btn-primary" type="submit">Next</button>
        </div>
    </form>
</div>

<% include ./partials/footer.ejs%>