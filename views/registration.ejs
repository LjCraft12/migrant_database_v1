<title> <%= title %> </title>

<!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"
      integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

<!-- Custom Stylesheets -->
<link rel="stylesheet" href="../assets/css/styles.css">

<% include ./partials/header.ejs%>

<div class="page-header text-center"><h1><%=header%></h1></div>

<div class="container">

    <form action="/registration" method="POST" id="signup" class="square">
        <img src="../assets/images/mmlogoblack.png" alt="">

        <div class="form-group">
            <label for=""> First Name:</label>
            <input type="text" placeholder="First Name" class="form-control" name="firstName" autofocus required>
        </div>

        <div class="form-group">
            <label for="">Last Name:</label>
            <input type="text" placeholder="Last Name" class="form-control" name="lastName" required>
        </div>

        <div class="form-group">
            <label for="">Phone:</label>
            <input type="text" placeholder="Phone" class="form-control" name="phone" required>
        </div>

        <div class="form-group">
            <label for="">E-mail:</label>
            <input type="text" placeholder="E-mail" class="form-control" name="email">
        </div>

        <input type="submit" value="Submit" class="btn btn-primary">

    </form>


</div>



<% include ./partials/footer.ejs%>